{
  "expo": {
    "name": "SignUpFactory",
    "slug": "SignUpFactory",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "SignUpFactory is an application where you can register and see the profile of other registered members.\n\nIt also allows you to enrich your profile with a photo to make other members jealous!",
    "githubUrl": "https://github.com/yungkittty/sign-up-factory"
  }
}
